package com.kh.operator;

public class C_Arithmetic {

	// 계산할 때 쓰이는 연산자
	public void method1() {
		int num1 = 10;
		int num2 = 3;

		System.out.println("num1 + nume2 = " + (num1 + num2)); // 13
		System.out.println("num1 - nume2 = " + (num1 - num2)); // 7
		System.out.println("num1 * nume2 = " + (num1 * num2)); // 30

	}

}
